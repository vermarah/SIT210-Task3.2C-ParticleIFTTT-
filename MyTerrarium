int led1 = D7; 
int light_sensor = A0;
int analogValue = 0;
int digitalValue;




void setup() {

  
  Particle.variable("Getting sunlight", &digitalValue, INT);
  
  Particle.variable("Getting Sunlight", &light_sensor, INT);
  Particle.variable("Getting Sunlight", &light_sensor, INT);
  
  pinMode(led1, OUTPUT);
  
  pinMode(light_sensor,INPUT);

}


void loop() {
  
  analogValue = analogRead(light_sensor);
 
 if (analogValue = (25))
 {
     digitalWrite(led1,HIGH);
     digitalWrite(led1,LOW);
     digitalWrite(led1,HIGH);
 }
 else
 {
     digitalWrite(led1,LOW);
     
 }

}
